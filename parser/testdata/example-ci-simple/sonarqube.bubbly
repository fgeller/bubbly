
// A pipeline_run resources executes a pipeline resource.
resource "pipeline_run" "sonarqube" {
    api_version = "v1"
    spec {
        // specify the name of the pipeline
        pipeline = "sonarqube"
        // specify the input required
        input "file" {
            value = "../sonarqube/sonarqube-example.json"
        }
    }
}
