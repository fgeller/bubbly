
input "translator_input" {
    default = ["a","b"]
}

resource "translator" "default" {
    apiVersion = "v1"
    spec {
        dynamic "data" {
            for_each = input.translator_input
            iterator = it
            labels = ["test_table", "table_${it.value}"]
            content {
                field "test_field" {
                    value = "This field exists in table_${it.value}"
                }
            }
        }
    }
}

output "first_output" {
    value = "wooooaaaw!"
}
